!!!
%html{:lang => :zh}
  %head
    %meta{:'http-equiv' => 'Content-Type', :content => 'text/html; charset=utf-8'}
    %meta{:name => 'viewport', :content => 'width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no'}

    %title 后台管理系统
    = csrf_meta_tags
    = stylesheet_link_tag 'bank_manage', :media => 'screen'
    = javascript_include_tag 'bank_manage'

  %body.page-manage
    = render :partial => "/layouts/row_signed_in_nav"

    .row
      .col-sm-3.col-md-2
        .sidebar-nav
          = simple_navbar(:bank_manage, ul_class: 'nav nav-pills nav-stacked')
      .col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2
        .row
          = yield
