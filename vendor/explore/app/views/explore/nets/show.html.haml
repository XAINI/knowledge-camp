:ruby
  tutorials_mock()

.net
  = render_cell 'explore/net', :header, @net
  = render_cell 'explore/net', :tab, @net, :tutorials, self

  .col-phone-12
    .tutorials
      - @tutorials.each do |tutorial|
        .tutorial
          %a.box{:href => "/explore/tutorials/#{tutorial.id}"}
            .img= fit_image tutorial.img, :width => '100%', :height => '100%'
            .title= tutorial.title
            .desc= tutorial.desc

